{
  "name": "charleston-fun-park",
  "description": "Charleston Fun Park",
  "private": false,
  "version": "0.0.1",
  "author": "Kenny Elshoff <me@kennyelshoff.com>",
  "bugs": {
    "url": "https://github.com/reallybadnews/charleston-fun-park/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.10",
    "@mdx-js/mdx": "^1.6.5",
    "@mdx-js/react": "^1.6.5",
    "framer-motion": "^2.0.0-beta.73",
    "gatsby": "^2.23.1",
    "gatsby-background-image": "^1.1.1",
    "gatsby-image": "^2.4.6",
    "gatsby-plugin-breakpoints": "^1.1.2",
    "gatsby-plugin-layout": "^1.3.3",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-mdx": "^1.2.14",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-react-helmet": "^3.3.3",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-source-contentful": "^2.3.14",
    "gatsby-source-filesystem": "^2.3.10",
    "gatsby-transformer-remark": "^2.8.14",
    "gatsby-transformer-sharp": "^2.5.4",
    "raam": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mdx": "^1.7.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "netlify-cli": "^2.52.0",
    "prettier": "^2.0.4",
    "prettier-eslint": "^10.1.1",
    "prettier-eslint-cli": "^5.0.0",
    "prop-types": "^15.7.2",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-webpack-plugin": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,scss,md,mdx,graphql}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "homepage": "https://github.com/reallybadnews/charleston-fun-park#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reallybadnews/charleston-fun-park.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "dev-m": "gatsby develop --host $(ifconfig | awk '/inet 192\\.168\\.[0-9]+\\.[0-9]+/{print $2}') --port 8000",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "eslint": "eslint --ext=js,jsx src",
    "eslint-fix": "eslint --ext=js,jsx src --fix",
    "format": "prettier --write \"{,!(node_modules)/**/}*.{js,jsx,mdx}\"",
    "lint": "npm run eslint && npm run stylelint",
    "lint-staged": "lint-staged",
    "netlify:login": "netlify login",
    "netlify:deploy": "netlify deploy -d public -p",
    "serve": "gatsby serve",
    "stylelint": "stylelint --config .stylelintrc.json src/**/*.{js,jsx}",
    "stylelint-fix": "stylelint --fix --config .stylelintrc.json src/**/*.{js,jsx}",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  }
}
