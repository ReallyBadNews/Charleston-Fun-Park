{
  "name": "charleston-fun-park",
  "description": "Charleston Fun Park",
  "private": false,
  "version": "2.0.0",
  "author": "Kenny Elshoff <me@kennyelshoff.com>",
  "bugs": {
    "url": "https://github.com/reallybadnews/charleston-fun-park/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "dev-m": "gatsby develop --host $(ifconfig | awk '/inet 192\\.168\\.[0-9]+\\.[0-9]+/{print $2}') --port 8000",
    "serve": "NODE_ENV=production gatsby serve",
    "format": "prettier --write \"{,!(node_modules)/**/}*.{js,jsx,ts,tsx,mdx}\"",
    "lint": "pnpm lint:ts && pnpm lint:es",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint '**/*.{js,jsx,ts,tsx}' --ignore-pattern 'node_modules/' --ignore-pattern '.cache/' --ignore-pattern 'public/'",
    "lint:es:fix": "eslint '**/*.{js,jsx,ts,tsx}' --ignore-pattern 'node_modules/' --ignore-pattern '.cache/' --ignore-pattern 'public/' --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@formium/client": "^0.1.4",
    "@formium/react": "^0.2.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "framer-motion": "^4.1.17",
    "gatsby": "^5.13.3",
    "gatsby-plugin-contentful-optional-fields": "^2.2.2",
    "gatsby-plugin-facebook-pixel": "^1.0.8",
    "gatsby-plugin-fontawesome-css": "^1.2.0",
    "gatsby-plugin-google-analytics": "^5.13.1",
    "gatsby-plugin-google-tagmanager": "^5.13.1",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-layout": "^4.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-mdx": "^2.14.0",
    "gatsby-plugin-react-helmet": "^6.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-plugin-theme-ui": "^0.11.3",
    "gatsby-plugin-typescript": "^5.13.1",
    "gatsby-remark-images-contentful": "^6.13.1",
    "gatsby-source-contentful": "^8.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-source-formium": "^0.2.0",
    "gatsby-transformer-sharp": "^5.13.1",
    "gbimage-bridge": "^0.2.2",
    "google-map-react": "^2.2.1",
    "raam": "^0.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "theme-ui": "^0.11.3",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@formium/types": "^0.1.3",
    "@types/google-map-react": "^2.1.10",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-preset-gatsby": "^3.13.1",
    "csstype": "^3.1.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-mdx": "^1.15.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-root-import": "^2.0.9",
    "husky": "^7.0.2",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "prop-types": "^15.8.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,md,mdx,graphql}": [
      "eslint --cache --fix"
    ]
  },
  "homepage": "https://github.com/reallybadnews/charleston-fun-park#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reallybadnews/charleston-fun-park.git"
  }
